{ FSRNDe  File Server random access }PROCEDURE new_blk(VAR fbk:FCB); EXTERNAL;PROCEDURE Update_blk(VAR fbk:Fcb;ptr:Dbl_word); EXTERNAL;   {Sets <error> }PROCEDURE Spare_FCB(VAR fcb_no:Byte); EXTERNAL; {Sets <error> }PROCEDURE Open_in(VAR fname:Min_string;VAR new:FCB;write:Boolean); EXTERNAL;{Sets <error> }PROCEDURE Open_out(VAR fname:Min_string;VAR new:FCB;init_ext:Dbl_word); EXTERNAL; {Sets <error> }PROCEDURE Close(VAR fblk:FCB); EXTERNAL; {Sets <error> }PROCEDURE X_open; EXTERNAL;PROCEDURE X_close; EXTERNAL;PROCEDURE B_get; EXTERNAL;PROCEDURE B_put; EXTERNAL;PROCEDURE MB_get; EXTERNAL;PROCEDURE MB_put; EXTERNAL;PROCEDURE Eof; EXTERNAL;PROCEDURE Rd_rnd_info; EXTERNAL;PROCEDURE Wr_rnd_info; EXTERNAL;