{ FSRND  File Server Random access }FUNCTION Cache_entry(desired:cache_descrip):BYTE;{ Brings the requested file segment into memory and returns the entry used }VAR a:integer;BEGINFOR a:=1 TO cache_size DO  IF cache_cont.age<255 THEN cache_cont.age:=succ(cache_cont.age);a:=1WHILE cache_cont[a].?????<>desired AND a=<cache_size DO a:=a+1;IF cache_cont[a].????=desired THEN BEGIN  cache_cont[a].age:=0;  cache_entry:=a;  {Entry found already in cache, so set age to best value, and return entry}END ELSE BEGIN  oldest:=0; {Find oldest entry in cache}  old_age:=0;  FOR a:=1 TO cache_size DO BEGIN    ent_age:=cache_cont[a].age;    IF ent_age>old_age THEN BEGIN old_age:=ent_age; oldest:=a END  ENDEND;