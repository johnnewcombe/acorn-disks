{ FSWILDe  File Server wildcard-able operations }PROCEDURE Star_access(VAR real_name:Min_string); EXTERNAL;PROCEDURE Star_acnt(VAR cmd_text:Min_string); EXTERNAL;PROCEDURE Star_rename(VAR cmd_text:Min_string); EXTERNAL;PROCEDURE Delete(dir_DDA:Disc_address;VAR fname:Min_string;d_flags:del_flags); EXTERNAL;{Sets <error>, corrupts <c_ent>}PROCEDURE Wild(DDA:Disc_address;VAR fname:Min_string;  PROCEDURE do_it(DDA:disc_address;VAR soft_err:Boolean)); EXTERNAL;{Sets <error>, corrupts <c_ent>}{<Do_it> sets <error>, uses <c_ent> as input param, & may corrupt it}