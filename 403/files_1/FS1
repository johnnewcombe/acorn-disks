{File FS1}ECOINIT;writeln(' PSJ File Server version ',verA:1,'.',verB:1,verC:1);writeln;REPEAT {Main operating loop}RXSET(blockno,cmdport,0,ADDR2(mainRX),min_size+5);REPEAT{Idle time in here!}UNTIL RXREAD(blockno,dummybyte,dummybyte,client_stn,dummy);WRITELN('FN=',mainRX.FN);WRITELN('Reply port=',mainRX.reply);CASE mainRX.fn OF  0:starcmd;  1:Save;  2:load; 18:Err_message(123); 23:Log_off;ELSE:txerr('Invalid FN code',133)END;{of case}UNTIL FALSE;