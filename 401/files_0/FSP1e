{ FSP1e  File Server Procedures & Functions }PROCEDURE Wrt_num(a:Word); EXTERNAL;PROCEDURE stnS(stn:Word;VAR dumS:Min_string); EXTERNAL;PROCEDURE Hex_word(w:Word); EXTERNAL;PROCEDURE Hex_print(d:dbl_word); EXTERNAL;PROCEDURE Err_message; EXTERNAL; {uses <error>}Function Fcb_no(handle:Byte):nibble; EXTERNAL; {sets <error>}FUNCTION Owner(main,aux:Byte):Boolean; EXTERNAL;PROCEDURE Check_dir(offset:Byte); EXTERNAL;PROCEDURE Check_not_dir(offset:Byte); EXTERNAL;FUNCTION Check_open(DDA:Disc_address;look_for:fcb_flags):Boolean;  EXTERNAL; {Sets <aux_error> but not <error> }FUNCTION Same_DDA(x,y:Disc_address):Boolean; EXTERNAL;FUNCTION Check_flags(VAR test:fcb;r_w:fcb_flags):Boolean; EXTERNAL;PROCEDURE Info_string(VAR ent:Ent_type;address:Word;VAR len:Byte); EXTERNAL;PROCEDURE Add_no_wrap(change:Word;VAR counter:Word); EXTERNAL;